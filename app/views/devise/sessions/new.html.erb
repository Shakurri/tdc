<div class="backg_top">
  <p class="log_title">> Log In.</p>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="log_box">
    <%= f.label :email,"> Email-Address." %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" ,:class=>"log_form"%>
  </div>

  <div class="log_box">
    <%= f.label :password ,"> LogIn-Password."%><br />
    <%= f.password_field :password, autocomplete: "off" ,:class=>"log_form"%>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="log_box_low">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me ,"Remember Me."%>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log In.",:class=>"log_but"%>
  </div>
<% end %>

<%= render "devise/shared/links"%>


