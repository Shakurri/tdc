<div class="backg_top">
  <p class="log_title">> Sign Up.</p>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="log_box">
    <%= f.label :email ,"> Email-Address."%><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" ,:class=>"log_form"%>
  </div>

 <div class="log_box">
    <%= f.label :username ,"> User Name."%><br />
    <%= f.text_field :username, autofocus: true, autocomplete: "username" ,:class=>"log_form"%>
  </div>

  <div class="log_box">
    <%= f.label :password ,"> Password."%>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off" ,:class=>"log_form"%>
  </div>

  <div class="log_box">
    <%= f.label :password_confirmation ,"> Re-Password."%><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" ,:class=>"log_form"%>
  </div>

  <div class="actions">
    <%= f.submit "Sign Up." ,:class=>"log_but"%>
  </div>
<% end %>

<%= render "devise/shared/links"%>

</div>
